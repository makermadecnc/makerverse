    "graphql": "yarn graphql-codegen --watch --config ./codegen.yml",
    "start": "NODE_OPTIONS=\"--trace-warnings\" PORT=8001 react-scripts start",
    "build": "react-scripts build",
    "dev": "yarn run start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "link-ows": "yarn link \"@openworkshop/lib\" && yarn link \"@openworkshop/ui\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged",
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
